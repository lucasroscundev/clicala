generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @default(uuid())
  email           String   @unique
  name            String
  nickname        String
  picture         String
  emailVerified   Boolean
  givenName       String
  familyName      String
  isAuthUser      Boolean
  createdAt       DateTime  @default(now()) @map("created_at")
  updatedAt       DateTime? @updatedAt @map("updated_at")

  @@map("users")
}